<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <TextView
        android:id="@+id/question"
        style="@style/txt_question"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Duae vestigia subversa Seleucia quam Claudius Seleuci multa hanc monstrat antehac pristinae monstrat et vestigia Caesar antehac ?"

        />

    <view
        android:id="@+id/barchart"
        class="nantes_sqli.rhchain.utils.BargraphLayout"
        id="@+id/viewBarChart"
        layout_width="match_parent"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0.12" />
</LinearLayout>
